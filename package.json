{
  "name": "orip-blockchain-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "test": "yarn hardhat test",
    "compile": "yarn hardhat compile",
    "node": "yarn hardhat node",
    "clean": "yarn hardhat clean",
    "deploy": "yarn hardhat deploy",
    "verify": "yarn hardhat verify",
    "run:hardhat": "yarn hardhat run",
    "postinstall": "yarn run compile && yarn run typechain:generate",
    "typechain:generate": "yarn typechain --target ethers-v5 --out-dir src/typechain 'solidity/artifacts/contracts/**/+([a-zA-Z0-9_]).json'"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@openzeppelin/contracts": "^4.6.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "chai": "^4.3.6",
    "chai-ethers": "^0.0.1",
    "dotenv": "^16.0.0",
    "hardhat": "^2.9.3",
    "hardhat-deploy": "^0.11.4",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "ethers": "^5.6.4",
    "express": "^4.18.0",
    "morgan": "^1.10.0",
    "nft.storage": "^6.2.0"
  }
}
